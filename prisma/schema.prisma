generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id @default(autoincrement())
  nome     String
  cpf      String  @unique
  login    String  @unique
  senha    String
  criadoEm DateTime @default(now())
}

model companies {
  id       Int     @id @default(autoincrement())
  nome     String
  cnpj     String  @unique
  criadoEm DateTime @default(now())
}

model adm{
  id       Int     @id @default(autoincrement())
  login     String
  senha     String  @unique
}